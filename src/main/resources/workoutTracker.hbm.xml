<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
       
<hibernate-mapping>
	<class name="com.tracker.model.AddWorkoutModel" table="workout_collection">
	<id name="workoutId" column="workout_id">
		<generator class="assigned"></generator>
	</id>
		<property name="workoutTitle" column="workout_title"/>
		<property name="workoutNote" column="workout_note" />
		<property name="caloriesBurnt" column="calories_burn_per_min" />
		<property name="categoryId"  column="category_id"/>
	</class>
</hibernate-mapping>